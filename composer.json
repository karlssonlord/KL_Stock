    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "MageTest/Manager": "dev-Add-php-provider-with-faker-capabilites",
        "mockery/mockery": "0.9.*",
        "magento/core": "~1.9",
        "bragento/magento-composer-installer": "~1.1"
    },
    "authors": [
        {
            "name": "David Wickstr√∂m",
            "email": "davidwickstrom@gmail.com"
        }
